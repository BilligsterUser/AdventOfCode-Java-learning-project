{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^1.1.0",
    "@material-ui/styles": "^3.0.0-alpha.8",
    "classnames": "latest",
    "http-proxy-middleware": "^0.19.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-native": "^0.58.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "mocha --require @babel/register --require helpers.js --require dom.js --require ignore-styles --require babel-polyfill 'src/**/*.test.js'",
    "eject": "react-scripts eject",
    "eslint": "./node_modules/.bin/eslint --max-warnings 0 src",
    "eslintFix": "./node_modules/.bin/eslint --fix src",
    "build:jar": "react-scripts build && rm -rf ../src/main/resources/static && mv build ../src/main/resources/static"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.6.0",
    "sinon": "^7.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
